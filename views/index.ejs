  <script>

    var swiper = new Swiper(".mySwiper", {
      effect: "cards",
      grabCursor: true,
    });


    var typed = new Typed('.change-text', {
      strings: ['Security Guard', 'Gunman', 'Doorman', 'Supervisor', 'Driver', 'Electrician', 'Liftman', 'Housekeeping', 'Plumber'],
      typeSpeed: 30,
      showCursor: false,
      loop: true,
      loopCount: Infinity,
      backDelay: 700
    });


    Shery.textAnimate("#top-hero h1", {
      style: 2,
      y: 50,
      delay: 0.1,
      duration: 2,
      ease: "cubic-bezier(0.23, 1, 0.320, 1)",
      multiplier: 0.1,
    });

    const mediaQuery = window.matchMedia('(max-width: 1000px)');
    if (mediaQuery.matches) {
      console.log('Screen width is less than or equal to 768px');
    } else {
    Shery.mouseFollower({
      skew: true,
      ease: "cubic-bezier(0.23, 1, 0.320, 1)",
      duration: 1,
    });
    Shery.makeMagnet("#contact button", {
      ease: "cubic-bezier(0.23, 1, 0.320, 1)",
      duration: 1,
    });
    Shery.makeMagnet(".main-btn", {
      ease: "cubic-bezier(0.23, 1, 0.320, 1)",
      duration: 1,
    });

    Shery.imageEffect("#hal-up", {
      style: 6,
      debug: false,
      config: { "noiseDetail": { "value": 7.44, "range": [0, 100] }, "distortionAmount": { "value": 2.98, "range": [0, 10] }, "scale": { "value": 36.36, "range": [0, 100] }, "speed": { "value": 0.79, "range": [0, 1] }, "zindex": { "value": -9996999, "range": [-9999999, 9999999] }, "aspect": { "value": 2.3346001746851526 }, "ignoreShapeAspect": { "value": true }, "shapePosition": { "value": { "x": 0, "y": 0 } }, "shapeScale": { "value": { "x": 0.5, "y": 0.5 } }, "shapeEdgeSoftness": { "value": 0, "range": [0, 0.5] }, "shapeRadius": { "value": 0, "range": [0, 2] }, "currentScroll": { "value": 0 }, "scrollLerp": { "value": 0.07 }, "gooey": { "value": true }, "infiniteGooey": { "value": true }, "growSize": { "value": 4, "range": [1, 15] }, "durationOut": { "value": 1, "range": [0.1, 5] }, "durationIn": { "value": 1.5, "range": [0.1, 5] }, "displaceAmount": { "value": 0.5 }, "masker": { "value": false }, "maskVal": { "value": 1, "range": [1, 5] }, "scrollType": { "value": 0 }, "geoVertex": { "range": [1, 64], "value": 1 }, "noEffectGooey": { "value": true }, "onMouse": { "value": 0 }, "noise_speed": { "value": 0.2, "range": [0, 10] }, "metaball": { "value": 0.2, "range": [0, 2] }, "discard_threshold": { "value": 0.5, "range": [0, 1] }, "antialias_threshold": { "value": 0, "range": [0, 0.1] }, "noise_height": { "value": 0.5, "range": [0, 2] }, "noise_scale": { "value": 10, "range": [0, 100] } },
      gooey: true,
    });

    new SplitType('#hal-down h1')

    gsap.from('#hal-down h1 .line .word .char', {
      y: 10,
      borderBottom: "4px solid black",
      opacity: 0,
      stagger: .2,
      duraction: 2,
      scrollTrigger: {
        trigger: "#hal-down",
        start: "top 60%",
        end: "top 70%",
        scrub: 2,
        // markers: true
      }
    })



    new SplitType('#haright p')

    gsap.from('#haright p .line .word .char', {
      y: 10,
      borderBottom: "4px solid black",
      opacity: 0,
      stagger: .2,
      duraction: 2,
      scrollTrigger: {
        trigger: "#haright",
        start: "top 40%",
        end: "top 50%",
        scrub: 2,
        // markers: true
      }
    })


