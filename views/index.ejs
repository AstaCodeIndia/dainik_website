      borderBottom: "4px solid black",
      opacity: 0,
      stagger: .2,
      duraction: 2,
      scrollTrigger: {
        trigger: "#hs-left",
        start: "top 30%",
        end: "top 40%",
        scrub: 2,
        // markers: true
      }
